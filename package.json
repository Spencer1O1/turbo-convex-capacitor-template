{
  "name": "monorepo-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:capacitor": "pnpm --filter @template/capacitor dev",
    "dev:android": "pnpm --filter @solvix/capacitor cap:open:android",
    "dev:ios": "pnpm --filter @solvix/capacitor cap:open:ios",
    "dev:backend": "pnpm --filter @template/backend dev",
    "cap:sync": "pnpm --filter @solvix/capacitor cap:sync",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck",
    "build": "turbo run build",
    "test": "turbo run test",
    "clean": "git clean -xdf node_modules .turbo dist .expo .next .cache",
    "rename": "node scripts/rename-template.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "ultracite": "6.0.1",
    "turbo": "^2.5.5",
    "typescript": "~5.9.3"
  },
  "packageManager": "pnpm@10.22.0"
}
